{
    "multipart": [
        {   "apply": { "model": "block/fence/birch/birch_fence_post" }},
        {   "when": { "AND": [
                { "north": false },
                { "east": false },
                { "south": false },
                { "west": false }
            ]},
				"apply": { "model": "block/fence/birch/birch_fence_top" }
		},
		{	"when": { "OR": [
					{"north":true},
					{"south":true}
			]},
				"apply": { "model": "block/fence/birch/birch_fence_top" }
		},
		{	"when": { "OR": [
					{"west":true},
					{"east":true}
			]},
				"apply": { "model": "block/fence/birch/birch_fence_top", "y":90 }
		},
        {   "when": { "north": "true" },
            "apply": { "model": "block/fence/birch/birch_fence_side", "uvlock": false }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "block/fence/birch/birch_fence_side", "y": 90, "uvlock": false }	
        },
        {   "when": { "south": "true" },
            "apply": { "model": "block/fence/birch/birch_fence_side", "y": 180, "uvlock": false }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "block/fence/birch/birch_fence_side", "y": 270, "uvlock": false }
        }

    ]
}
