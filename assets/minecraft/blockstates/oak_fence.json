{
	"multipart": [
		{ "apply": { "model": "block/fence/oak/oak_fence_post" }},
		{ "when": { "north": "true" },
			"apply": { "model": "block/fence/oak/oak_fence_side", "uvlock": false }
		},
		{ "when": { "east": "true" },
			"apply": { "model": "block/fence/oak/oak_fence_side", "y": 90, "uvlock": false }
		},
		{ "when": { "south": "true" },
			"apply": { "model": "block/fence/oak/oak_fence_side", "x": 180, "uvlock": false }
		},
		{ "when": { "west": "true" },
			"apply": { "model": "block/fence/oak/oak_fence_side", "x": 180, "y": 90, "uvlock": false }
		}
	]
}
